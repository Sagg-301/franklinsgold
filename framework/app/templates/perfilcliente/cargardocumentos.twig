                    <small class="font-bold">Franklins Gold</small>
                </div>
                <div class="modal-body">
                    <form role="form" id="cargar_documentos_form">
                        <div class="row">

                            <div class="col-md-12 col-xs-12">
                                <div class="form-group">
                                    <label for="id_foto_documento" class="float-left">Subir fotografía del documento de identidad </label>
                                    <input type="file" class="form-control-file" name="foto_documento" id="id_foto_documento">
                                </div>
                            </div>

                            <div class="col-md-12 col-xs-12">
                                <div class="form-group">
                                    <label for="id_foto_pasaporte" class="float-left">Subir fotografía del pasaporte </label>
                                    <input type="file" class="form-control-file" name="foto_pasaporte" id="id_foto_pasaporte">
                                </div>
                            </div>

                            <div class="col-md-12 col-xs-12">
                                <div class="form-group">
                                    <label for="id_foto_rif" class="float-left">Subir fotografía del rif </label>
                                    <input type="file" class="form-control-file" name="foto_rif" id="id_foto_rif">
                                </div>
                            </div>

                            <div class="col-md-12 col-xs-12">
                                <div class="form-id_foto_ref1">
                                    <label for="id_foto_ref1" class="float-left">Subir fotografía de referencia bancaria </label>
                                    <input type="file" class="form-control-file" name="foto_ref1" id="id_foto_ref1">
                                </div>
                            </div>

                            <div class="col-md-12 col-xs-12">
                                <div class="form-group">
                                    <label for="id_foto_ref2" class="float-left">Subir fotografía de referencia bancaria </label>
                                    <input type="file" class="form-control-file" name="foto_ref2" id="id_foto_ref2">
                                </div>
                            </div>

                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Cerrar</button>
                    <button type="button" id="cargardocumentosbtn" class="btn btn-primary">Crear</button>
                </div>
            </div>
        </div>
</div>
